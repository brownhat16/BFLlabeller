{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.10",  // Use 3.10 instead of 3.12
        "installCommand": "pip install --no-cache-dir -r requirements.txt"  // Disable cache
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
